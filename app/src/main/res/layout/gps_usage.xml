<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/gps_latitude_tv"
        android:layout_width="124dp"
        android:layout_height="73dp"
        android:text="@string/gps_la"
        app:layout_constraintBottom_toTopOf="@+id/gps_longitude_tv"
        app:layout_constraintHorizontal_bias="0.477"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gps_longitude_tv"
        android:layout_width="122dp"
        android:layout_height="74dp"
        android:text="@string/gps_lo"
        app:layout_constraintBottom_toTopOf="@+id/gps_nowAddress_tv"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/gps_latitude_tv" />

    <TextView
        android:id="@+id/gps_nowAddress_tv"
        android:layout_width="122dp"
        android:layout_height="67dp"
        android:text="@string/gps_addr"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>